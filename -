  `supplier_total` decimal(8,2) NOT NULL DEFAULT 0.00,
  `total_platform_taxes` decimal(8,2) NOT NULL DEFAULT 0.00,
  `total_country_taxes` decimal(8,2) NOT NULL DEFAULT 0.00,
  `total_other_taxes` decimal(8,2) NOT NULL DEFAULT 0.00,
  `total_taxes` decimal(8,2) NOT NULL DEFAULT 0.00,
  `total_products` int(11) NOT NULL DEFAULT 0,
  `total_discount` decimal(10,2) NOT NULL DEFAULT 0.00,
  `status` enum('pending','accepted','rejected','paid','shipped','delivered','cancelled') NOT NULL DEFAULT 'pending',
  `order_type` enum('1','2') NOT NULL DEFAULT '1',
  `deleted_at` datetime DEFAULT NULL,
  `user_id` bigint(20) unsigned DEFAULT NULL,
  `supplier_id` bigint(20) unsigned DEFAULT NULL,
  `created_at` timestamp NULL DEFAULT NULL,
  `updated_at` timestamp NULL DEFAULT NULL,
  PRIMARY KEY (`id`),
  KEY `orders_user_id_foreign` (`user_id`),
  KEY `supplier_status_user_total_products_total` (`supplier_id`,`status`,`user_id`,`total_products`,`total`),
  CONSTRAINT `orders_supplier_id_foreign` FOREIGN KEY (`supplier_id`) REFERENCES `users` (`id`) ON DELETE SET NULL,
  CONSTRAINT `orders_user_id_foreign` FOREIGN KEY (`user_id`) REFERENCES `users` (`id`) ON DELETE SET NULL
